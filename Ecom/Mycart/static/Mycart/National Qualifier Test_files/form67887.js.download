function splituploadDocsString(stringToSplit, separator,elementToSearch) 
{
	var arrayOfStrings = stringToSplit.split(separator);
	var a=0;
	for (var i=0; i < arrayOfStrings.length; i++)
	{
		if(elementToSearch==arrayOfStrings[i])
		{
			a=a+1;
		}
	}
	return a;
}

