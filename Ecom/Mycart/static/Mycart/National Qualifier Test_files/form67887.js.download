	{
		alert("Please enter Email Address.");
		return false;
	}
	if(document.getElementById("txtcontact_no").value == '')
	{
		alert("Please enter Mobile No.");
		return false;
	}
    var a=document.getElementById('captchaService_imageKey').value;
    var b=document.getElementById('captchaService_answer').value;

	var formId=67887;
	var orgId=1016;
	init_captcha();
    if (b == '') 
	{
      alert("Please enter the Captcha.");
      return false;
    }
	else if(b!='')
	{
		var appSeqNo='0'; 
		var RollNo10th='0'; 
		var RollNo12th='0';


		if(document.getElementById('txtemail_add').value!=''){
			RollNo10th = document.getElementById('txtemail_add').value;
		}

		if(document.getElementById('txtcontact_no').value!='')
		{
			RollNo12th = document.getElementById('txtcontact_no').value;
		}

		var paradetails="txtemail_add,txtcontct_no,a,b,c,d";


    	var http_request=fnGetXMLHttpObject();
    	if(http_request!=null)
		{

			var pValidationServlet = "";
           	var pValidationServlet ="/EForms/eformactivationservlet";
			var actionId="ForgotPwd";
			var arrayArg = new Array(formId,orgId,RollNo10th,RollNo12th,'0','0','0',paradetails,http_request);
			var argNum=8;

			var parameterString= "actionId=" + actionId + "&argnum="+ argNum+"&captchaService_imageKey="+a+"&captchaService_answer="+b+"&appId=30";

			var parameters = new Array();

			for (var i=0; i < arrayArg.length; i++){
				if(arrayArg[i] != ''){
					parameterString += "&" + "args" +(i+1) +"=" + encodeURI(arrayArg[i]);
				}
			}

			http_request.open('POST',pValidationServlet,true);  
			http_request.onreadystatechange = function() {  fnPwdN1(http_request,button_id); }; 	 	                    
			http_request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");               
			http_request.send(parameterString);
		}

    }  
	else
	{
		alert("Please enter correct text shown in image");
