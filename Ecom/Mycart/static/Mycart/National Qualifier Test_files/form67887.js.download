/**************************doTaskInEdit************************************/
function doTaskInEdit()
{ 
	if($('input:input')){$('input:input').attr('oncopy','return false').attr('onpaste','return false').attr('autocomplete','off');}
	var product_name	=	$("#txtCcqtStream").val()
	$("#txtCcqtStream_h5").text($("#txtCcqtStream").val());
	var pro_name	=	$("#txtCcqtStream_h5").text();
	if(pro_name.indexOf("TGQT") != -1){
		$('#ctHeading').text("TCS iON Qualifier - TGQT selected variant");
		$('#ctLabel').text("TCS iON Qualifier - TGQT Stream");
	} else {
		$('#ctHeading').text("TCS iON Qualifier - CCQT selected variant");
		$('#ctLabel').text("TCS iON Qualifier - CCQT Stream");
	}
	
	/*
	Degree / Branch Array
	*/
	var sci_array		=	["Master of Science - M.Sc.","Bachelor of Science - B.Sc.","Master of Computer Applications - M.C.A.","Bachelor of Computer Applications - B.C.A.","Bachelor of Pharmacy - B.Pharm.","Master of Pharmacy - M.Pharm."];
	var sci_role_array	=	["Physics","Chemistry","Biology","Computer Science","Information Technology","Others"];
	
	var art_com_array	=	["Master of Arts - M.A.","Bachelor of Arts - B.A.","Master of Commerce - M.Com.","Bachelor of Commerce - B.Com."];
	var art_com_role_array = ["Economics","Accountancy","Statistics","Political Science","History","Sociology","Psychology","Geography","Philosophy","Human Resources","Operations","Marketing","Pharma","Others"];
	
	var engg_array		=	["Master of Engineering - M.E.","Bachelor of Engineering - B.E.","Master of Technology - M.Tech.","Bachelor of Technology - B.Tech."];
	var engg_role_array = ["Chemical","Civil","Electrical","Mechanical","Computer Science","Information Technology","Instrumentation","Electronics","Electronics and Telecom","Production","Industrial","Metallurgy","Aeronautical","Others"];
	
	var mgmt_array		=	["Master of Business Administration - MBA","Bachelor of Business Administration - BBA", "Post Graduate Diploma in Management - PGDM"];
	var mgmt_role_array = ["Marketing","Finance","Operations","Human Resources","Business Management","Others"];
	
	/*
		Science
	*/
	if(product_name == "TCS iON Qualifier - TGQT - Non Engineering Graduates - Science" || product_name == "TCS iON Qualifier - CCQT - Science Graduates"  || product_name == "TCS iON Qualifier - CCQT - Science Graduates")
	{
		$('#txtDiplomaDegree').empty();
		//$('#txtDiplomaDegree').append($("<option></option>").text("---Select---").val(""));
		
		$('#txtBranch').empty();
		//$('#txtBranch').append($("<option></option>").text("---Select---").val(""));
		for(var p=0; p < sci_array.length; p++){
			$('#txtDiplomaDegree').append($("<option></option>").val(sci_array[p]).attr('title',sci_array[p]).text(sci_array[p]));
		}
		//$('#txtDiplomaDegree').append($("<option></option>").text("Others").val("Others"));
		//$('#txtDiplomaDegree').append($("<option></option>").text("NA").val("NA"));
		$('#txtDiplomaDegree').selectpicker('refresh');
		
		for(var r=0; r < sci_role_array.length; r++){
			$('#txtBranch').append($("<option></option>").val(sci_role_array[r]).attr('title',sci_role_array[r]).text(sci_role_array[r]));
		}
		//$('#txtBranch').append($("<option></option>").text("NA").val("NA"));
		$('#txtBranch').selectpicker('refresh');
	}
	
	/*
