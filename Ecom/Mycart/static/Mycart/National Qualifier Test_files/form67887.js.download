function Year_Check(id,id1,statement){
	if($('#'+id1).val()!="" && $('#'+id).val()!="")
		{
			var AppliYear = '';
			var dateString = $('#'+id1).val();
			if(dateString.includes('/'))
			{
			var AppliDob=dateString.split('/');
			AppliYear = AppliDob[2];
			}
			else{
				AppliYear = dateString;
			}
			var EdateString = $('#'+id).val();
			if(AppliYear>=EdateString){
				alert(""+statement);
				$('#'+id).val("");
				$('#'+id).focus();
			} 
		}
}


