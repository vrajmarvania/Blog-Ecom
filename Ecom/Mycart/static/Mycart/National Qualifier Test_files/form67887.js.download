function mobile_no_check(id)
{
	if($("#"+id).val() != "")
	{
		if(trim($("#"+id).val()).length < 10 )
		{
			alert("Please enter 10 digit Mobile Number.");
			$("#"+id).focus();
			$("#"+id).val("");
			validate=false;
			return false;
		}
		var contact = $("#"+id).val();
		if((contact.charAt(0)>=0)&&(contact.charAt(0)<=5))
		{
			alert("Mobile number cannot begin with 0,1,2,3,4,5 or an alphabet.");
			$("#"+id).focus();
			$("#"+id).val("");
			validate=false;
			return false;
		}
		else
		{
			validate = true;
		}
		return validate;
	} 
}


