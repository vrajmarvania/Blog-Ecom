function CheckforOTP() {
    if ($('#txtMobileNo').val() == '' ) {
        alert('Enter Mobile number for OTP');
        return false;
    }
    if ($('#txtEmail').val() == '') {
        alert('Enter Email Addres for OTP');
        return false;
    } else {
		sendOTPSMS();
        return true;
    }	
}


