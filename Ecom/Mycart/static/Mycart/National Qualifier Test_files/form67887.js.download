function drop_select_if(id)
{
	$('#'+id).parent().find('.errortxt').css('display', 'inline');
	$('#'+id).next().addClass('borderredcolor1');
	$('#'+id).next().find('button').addClass('borderredcolor');
	$(window).scrollTop($('[data-id="'+id+'"]').offset().top);
	if(id == 'txtDistrict1'){ 
		$('html, body').animate({
			scrollTop: $(".cityclass").offset().top
		}, 100);
	} else if(id == 'txtGender'){ 
		$('html, body').animate({
			scrollTop: $(".genderclass").offset().top
		}, 100);
	}
}
function drop_select_else(id)
{
	$('#'+id).parent().find('.errortxt').css('display', 'none');
	$('#'+id).next().removeClass('borderredcolor1');
	$('#'+id).next().find('button').removeClass('borderredcolor');
}


function input_if(id)
{
	$('#'+id).parent().find('.errortxt').css('display', 'inline');
	$('#'+id).addClass('borderredcolor');

}
function input_else(id)
{
	$('#'+id).parent().find('.errortxt').css('display', 'none');
	$('#'+id).removeClass('borderredcolor');
	$('#'+id).css('color','#333');
}
