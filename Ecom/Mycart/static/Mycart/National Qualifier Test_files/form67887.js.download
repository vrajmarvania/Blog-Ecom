function isScore1(key,event){
 	var keyCode;
	var isIE;

	if(navigator.appName == "Microsoft Internet Explorer" || navigator.appName == "Netscape"){  
		keyCode = event.keyCode;
		
		isIE = 1;
		if(navigator.appName == "Netscape"){
		    keyCode = event.charCode;		   
			isIE = 0;
		}
	}else{
		keyCode = event.charCode;
		isIE = 0;
	}
		  
			if(isIE == 1 ){
			if(!((keyCode >= 48 && keyCode <= 57)|| keyCode == 65 || keyCode == 78 ))
			{
			return false;
				}
			}
			else{
			if(!((keyCode >= 48 && keyCode <= 57) || keyCode == 0 || keyCode == 65 || keyCode == 78))
			{
			
				event.preventDefault();								
			}
						
	  }
}
function CheckSingleDot(id,suffix) {
		var countDot = 0;
		var val=document.getElementById(id).value;
		if (val.charAt("0") == ".") {
			alert("Incorrect Number Format, Please enter correct Number in " + suffix + ".");
			$("#" + id).val("");
			$("#" + id).focus();
		}
		if (val[val.length - 1] == ".") {
			alert("Incorrect Number Format, Please enter correct Number in " + suffix + ".");
			$("#" + id).val("");
			$("#" + id).focus();
		}
		for (var i = 0; i < val.length; i++) {
			if (val.charAt(i) == '.') {
				countDot++;
			}
		}
		if (countDot > 1) {
			alert('Incorrect Format of Number, Please enter correct Number with single "." in ' + suffix + '.');
			$("#" + id).val("");
			$("#" + id).focus();
		}
	}

function isScore(key,event) {
 	var keyCode;
	var isIE;

	if(navigator.appName == "Microsoft Internet Explorer" || navigator.appName == "Netscape"){  
		keyCode = event.keyCode;
		
		isIE = 1;
		if(navigator.appName == "Netscape"){
		    keyCode = event.charCode;		   
			isIE = 0;
		}
	}else{
		keyCode = event.charCode;
		isIE = 0;
	}
		  
			if(isIE == 1 ){
			if(!((keyCode >= 48 && keyCode <= 57) || keyCode==46 || keyCode == 65 || keyCode == 78 ))
			{
			event.returnValue = false;
				}
			}
			else{
			if(!((keyCode >= 48 && keyCode <= 57) || keyCode == 0 || keyCode==46 || keyCode == 65 || keyCode == 78 ))
			{
			
				event.preventDefault();								
			}
						
	  }
}

function checkcity(elemt) {
	for (i = 1; i <= 3; i++) {
		var otherElemt = document.getElementById("txtLangKnown" + i);
		if (otherElemt.id != elemt.id && elemt.options[elemt.selectedIndex].value != "NA" && elemt.options[elemt.selectedIndex].value != ""	&& otherElemt.options[otherElemt.selectedIndex].value != "NA"	&& otherElemt.options[otherElemt.selectedIndex].value == elemt.options[elemt.selectedIndex].value) {
			alert("This option is already selected. Please select a different option.");
			elemt.selectedIndex = 0;
			break;
		}
	}
}



function SeatCountStateCity(ajaxfor) 
{
 		  var results='';
		  $.ajax({
			type: "POST",
			url: "/EForms/cdmsservlet", //Your servlet name.
			data: {
				actionId: "NQTSearch",
				orgId: 1016,
				Formid:"67887",
				State: "",
				City: "",
				Institute:"",
				Pin: "",
				preferenceState: $('#txtStateCenterChoice1').val(),
                preferenceCity: $('#CenterChoice1').val(),
				ajaxFor: ajaxfor,
				argnum:0,
			},
			dataType: "json",
			success: function (result) {
			if(ajaxfor == 'PreferenceState')
			{
				$('#txtStateCenterChoice1').empty();
				$('#txtStateCenterChoice1').append($("<option></option>").text("Select State Preferences").val(""));
				for(var i=0;i<result.PreferenceState_data.length;i++){
					 $('#txtStateCenterChoice1').append($("<option></option>").val(result.PreferenceState_data[i].PreferenceState).attr('title',result.PreferenceState_data[i].PreferenceState).text(result.PreferenceState_data[i].PreferenceState));
				}
				$('#txtStateCenterChoice1').selectpicker('refresh');
			}
			if(ajaxfor == 'PreferenceCity')
			{
				$('#CenterChoice1').empty();
				$('#CenterChoice1').append($("<option></option>").text("Select City Preferences").val(""));
				for(var i=0;i<result.PreferenceCity_data.length;i++){
					$('#CenterChoice1').append($("<option></option>").val(result.PreferenceCity_data[i].PreferenceCity).attr('title',result.PreferenceCity_data[i].PreferenceCity).text(result.PreferenceCity_data[i].PreferenceCity));
				}
				$('#CenterChoice1').selectpicker('refresh');
			}
			if(ajaxfor == 'AvailableSeat')
            {
                //alert(result.SeatData[0].isAvailableSeat +" / "+result.SeatData[0].AvailableSeat);
				if(result.SeatData[0].AvailableSeat == 0)
				{
					//alert("Sitting capacity of the centre you selected is full. Please select another nearby centre.");
					$(".capacity").show();
					$('#CenterChoice1').val("");
					$('#CenterChoice1').selectpicker('refresh');
				}
				else
				{
					$(".capacity").hide();
				}	
            }
			},
			error: function (result) {
				//alert("Error: "+result);
				$("#searchedData").html('No Result Found');
			}
		});
   //} 
}


function ExamMode()
{
	var selValueByClass = $(".moe:checked").val();
    if(selValueByClass == 'In - Centre')
	{
		$(".prefincenter").show();
		SeatCountStateCity('PreferenceState');
	}
	else
	{
		$(".prefincenter").hide();
		$("#txtStateCenterChoice1").val("");
		$("#txtCenterChoice1").val("");
	}
	$("#txtExamMode").val(selValueByClass);
}

function ProgTest111()
{
	var selValueByClass = $(".prog:checked").val();
	if(selValueByClass == 'Yes')
	{
		$('#checkbox1').prop('checked', true);
		$("#checkbox1").attr("disabled", true);
		//$("#txtSubscribedProduct").val();
	} else {
		$('#checkbox1').prop('checked', false);
		$("#checkbox1").attr("disabled", false);
		//$("#txtSubscribedProduct").val($("#txtVariantCode").val().trim());
