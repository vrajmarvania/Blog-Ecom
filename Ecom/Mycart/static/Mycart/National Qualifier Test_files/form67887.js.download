function showItNonit(showcls,showId)
{
	$('#fragment-28').css('display','inline-block');
	var selVal = $("#"+showId).val();
	if(selVal == 'Yes' || selVal == "Yes - Others")
	{
		$("."+showcls).show();
		$("."+showcls+" .form-control").val("");
		if(showcls == 'showSubIt')
		{
			$("#txtItCertYear").val("");
			$('#txtItCertYear').selectpicker('refresh');
			$('#txtItCertYear').selectpicker({
		   noneSelectedText:"---Select---"
			});
			
		}
		if(showcls == 'showSubNonIt')
		{
			$("#txtNonItCertYear").val("");
			$('#txtNonItCertYear').selectpicker('refresh');
			$('#txtNonItCertYear').selectpicker({
		   noneSelectedText:"---Select---"
			});
			$("#txtNonItCertname1").val("");
			$('#txtNonItCertname1').selectpicker('refresh');
			$(".showSubNonIt1").hide();
		}		
	}
	else if(selVal == 'Yes - "Business English Certificate (BEC)" Certified')
	{
		$(".showSubNonIt1").show();
		$(".certyear").show();
		$("#txtNonItCertname").val("");
		$("#txtNonItCertYear").val("");
		$("#txtNonItCertname1").val("");
		$('#txtNonItCertYear').selectpicker('refresh');
		$('#txtItCertYear').selectpicker({
		   noneSelectedText:"---Select---"
			});
		$('#txtNonItCertname1').selectpicker('refresh');
		$(".nametext").hide();
	}
	else if(selVal == "No" || selVal == "")
	{
		console.log("This is from function");
		if(showcls == 'showSubIt')
		{
			$("."+showcls).hide();
			$("#txtItCertname").val("");
			$("#txtItCertYear").val("");
			$('#txtItCertYear').selectpicker('refresh');
		}
		if(showcls == 'showSubNonIt')
		{
			console.log("This is from function NonIt");
			$("#txtNonItCertname1").val("");
			$('#txtNonItCertname1').selectpicker('refresh');
			$("#txtNonItCertname").val("");
			$("#txtNonItCertYear").val("");
			$('#txtNonItCertYear').selectpicker('refresh');
			$(".showSubNonIt").hide();
			$(".showSubNonIt1").hide();
		}	
	}
	
	if(selVal == '')
	{
		$("."+showcls+" .form-control").val("");
		if(showcls == 'showSubIt')
		{
			$("#txtItCertYear").val("");
			$('#txtItCertYear').selectpicker('refresh');
		}
		if(showcls == 'showSubNonIt')
		{
			$("#txtNonItCertYear").val("");
			$('#txtNonItCertYear').selectpicker('refresh');
			$('#txtNonItCertname1').selectpicker('refresh');
		}	
	}
}
	
function DobDDMMYYYY()
{
	dob=document.getElementById('appDob').value;
	if(dob!="")
	{
		months = {'Jan': '01', 'Feb': '02', 'Mar': '03', 'Apr': '04', 'May': '05','Jun': '06', 'Jul': '07', 'Aug': '08', 'Sep': '09', 'Oct': '10', 'Nov': '11','Dec': '12'};

		split = dob.split('/');
		dob=[split[0], months[split[1]],  split[2]].join('/');
		year=split[2];
//		console.log(year);
		document.getElementById('appDob').value=dob;
	}
}

